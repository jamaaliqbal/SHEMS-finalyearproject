lass=\"col-md-6\">\r\n                <div class=\"card shadown-sm mb-4\">\r\n                    <div class=\"card-body\">\r\n                        <h4 class=\"card-title text-dark\">Solar Energy Data</h4>\r\n                        <p><strong>Current Solar Power:</strong> {{ solarEnergyData.acpower }}kWh</p>\r\n                        <p><strong>Current Solar Energy:</strong> {{ solarEnergyData.yieldtoday }}kWh</p>\r\n                        <p><strong>Current Solar Power Percentage:</strong> {{ solarEnergyData.soc }}%</p>\r\n                        <p><strong>Current Solar Energy Percentage:</strong> {{ solarEnergyData.uploadTime }}%</p>\r\n                        <button @click=\"loadSolarData\" class=\"btn btn-primary mt-3\">Refresh data</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        \r\n        <!-- <button @click=\"loadSolarData\">Load Solar button</button> -->\r\n    </div>\r\n    <!-- <div v-if=\"solarEnergyData\">\r\n        <h2>Solar Energy Data</h2>\r\n        <p>Current Solar Power: {{ solarEnergyData.acpower }}kWh</p>\r\n        <p>Current Solar Energy: {{ solarEnergyData.yieldtoday }}kWh</p>\r\n        <p>Current Solar Power Percentage: {{ solarEnergyData.soc }}%</p>\r\n        <p>Current Solar Energy Percentage: {{ solarEnergyData.uploadTime }}%</p>\r\n    </div> -->\r\n\r\n</template>\r\n\r\n<script>\r\nimport apiClient from '@/services/api';\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            energyData: [],\r\n            solarEnergyData: null,\r\n            realTimeData: null,\r\n        };\r\n    },\r\n    mounted() {\r\n        apiClient.get('energy-data/')\r\n            .then(response => {\r\n                this.energyData = response.data;\r\n            })\r\n            .catch(error => {\r\n                console.error('Error fetching data:', error);\r\n            });\r\n    },\r\n    methods: {\r\n        loadSolarData() {\r\n            console.log(\"TEST\")\r\n            axios.get('http://127.0.0.1:8000/api/solax-data/', {\r\n                wifiSn: 'SV8RYX9GZU',\r\n            }).then((response) => {\r\n                console.log(\"Solar Data:\", response.data);\r\n                this.solarEnergyData = response.data.result;\r\n            }).catch((error) => {\r\n                console.error('Error fetching solar data:', error);\r\n            });\r\n        }\r\n    }\r\n};\r\n</script>\r\n"],"mappings":"AA4CA,OAAOA,SAAQ,MAAO,gBAAgB;AACtC,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAE,EAAE;MACdC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNN,SAAS,CAACO,GAAG,CAAC,cAAc,EACvBC,IAAI,CAACC,QAAO,IAAK;MACd,IAAI,CAACN,UAAS,GAAIM,QAAQ,CAACP,IAAI;IACnC,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;IAChD,CAAC,CAAC;EACV,CAAC;EACDE,OAAO,EAAE;IACLC,aAAaA,CAAA,EAAG;MACZF,OAAO,CAACG,GAAG,CAAC,MAAM;MAClBd,KAAK,CAACM,GAAG,CAAC,uCAAuC,EAAE;QAC/CS,MAAM,EAAE;MACZ,CAAC,CAAC,CAACR,IAAI,CAAEC,QAAQ,IAAK;QAClBG,OAAO,CAACG,GAAG,CAAC,aAAa,EAAEN,QAAQ,CAACP,IAAI,CAAC;QACzC,IAAI,CAACE,eAAc,GAAIK,QAAQ,CAACP,IAAI,CAACe,MAAM;MAC/C,CAAC,CAAC,CAACP,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACtD,CAAC,CAAC;IACN;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}